<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tencent.wxcloudrun.dao.MerchantVoucherManagerMapper">
    <resultMap type="com.tencent.wxcloudrun.model.MerchantVoucherManager" id="MerchantVoucherManagerMap">
        <result property="id" column="id" jdbcType="VARCHAR"/>
        <result property="voucherName" column="voucher_name" jdbcType="VARCHAR"/>
        <result property="voucherAmt" column="voucher_amt" jdbcType="VARCHAR"/>
        <result property="belongMerchantId" column="belong_merchant_id" jdbcType="VARCHAR"/>
        <result property="voucherDes" column="voucher_des" jdbcType="VARCHAR"/>
        <result property="isShow" column="is_show" jdbcType="BOOLEAN"/>
    </resultMap>

    <!-- 通过ID查询单条数据 -->
    <select id="queryById" resultMap="MerchantVoucherManagerMap">
        select
            id,voucher_name,voucher_amt,belong_merchant_id,voucher_des,is_show
        from merchant_voucher_manager
        where id = #{id}
    </select>

    <!--分页查询指定行数据-->
    <select id="queryAllByLimit" resultMap="MerchantVoucherManagerMap">
        select
        id,voucher_name,voucher_amt,belong_merchant_id,voucher_des,is_show
        from merchant_voucher_manager
        <where>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
            <if test="voucherName != null and voucherName != ''">
                and voucher_name = #{voucherName}
            </if>
            <if test="voucherAmt != null and voucherAmt != ''">
                and voucher_amt = #{voucherAmt}
            </if>
            <if test="belongMerchantId != null and belongMerchantId != ''">
                and belong_merchant_id = #{belongMerchantId}
            </if>
            <if test="voucherDes != null and voucherDes != ''">
                and voucher_des = #{voucherDes}
            </if>
            <if test="isShow != null and isShow != ''">
                and is_show = #{isShow}
            </if>
        </where>
        limit #{pageable.offset}, #{pageable.pageSize}
    </select>
    <!--分页查询指定行数据-->
    <select id="queryAll" resultMap="MerchantVoucherManagerMap">
        select
        id,voucher_name,voucher_amt,belong_merchant_id,voucher_des,is_show
        from merchant_voucher_manager
        <where>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
            <if test="voucherName != null and voucherName != ''">
                and voucher_name = #{voucherName}
            </if>
            <if test="voucherAmt != null and voucherAmt != ''">
                and voucher_amt = #{voucherAmt}
            </if>
            <if test="belongMerchantId != null and belongMerchantId != ''">
                and belong_merchant_id = #{belongMerchantId}
            </if>
            <if test="voucherDes != null and voucherDes != ''">
                and voucher_des = #{voucherDes}
            </if>
            <if test="isShow != null and isShow != ''">
                and is_show = #{isShow}
            </if>
        </where>
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from merchant_voucher_manager
        <where>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
            <if test="voucherName != null and voucherName != ''">
                and voucher_name = #{voucherName}
            </if>
            <if test="voucherAmt != null and voucherAmt != ''">
                and voucher_amt = #{voucherAmt}
            </if>
            <if test="belongMerchantId != null and belongMerchantId != ''">
                and belong_merchant_id = #{belongMerchantId}
            </if>
            <if test="voucherDes != null and voucherDes != ''">
                and voucher_des = #{voucherDes}
            </if>
            <if test="isShow != null and isShow != ''">
                and is_show = #{isShow}
            </if>
        </where>
    </select>

    <!--新增数据-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into merchant_voucher_manager(id,voucher_name,voucher_amt,belong_merchant_id,voucher_des,is_show)
        values (#{id},#{voucherName},#{voucherAmt},#{belongMerchantId},#{voucherDes},#{isShow})
    </insert>

    <!-- 批量新增数据 -->
    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into merchant_voucher_manager(id,voucher_name,voucher_amt,belong_merchant_id,voucher_des,is_show)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.id},#{entity.voucherName},#{entity.voucherAmt},#{entity.belongMerchantId},#{entity.voucherDes},#{entity.isShow})
        </foreach>
    </insert>

    <!-- 批量新增或按主键更新数据 -->
    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into merchant_voucher_manager(id,voucher_name,voucher_amt,belong_merchant_id,voucher_des,is_show)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.id},#{entity.voucherName},#{entity.voucherAmt},#{entity.belongMerchantId},#{entity.voucherDes},#{entity.isShow})
        </foreach>
        on duplicate key update
        id=values(id),
        voucher_name=values(voucher_name),
        voucher_amt=values(voucher_amt),
        belong_merchant_id=values(belong_merchant_id),
        voucher_des=values(voucher_des),
        is_show=values(is_show)
    </insert>

    <!-- 更新数据 -->
    <update id="update">
        update merchant_voucher_manager
        <set>
            <if test="id != null and id != ''">
                id = #{id},
            </if>
            <if test="voucherName != null and voucherName != ''">
                voucher_name = #{voucherName},
            </if>
            <if test="voucherAmt != null and voucherAmt != ''">
                voucher_amt = #{voucherAmt},
            </if>
            <if test="belongMerchantId != null and belongMerchantId != ''">
                belong_merchant_id = #{belongMerchantId},
            </if>
            <if test="voucherDes != null and voucherDes != ''">
                voucher_des = #{voucherDes},
            </if>
            <if test="isShow != null and isShow != ''">
                is_show = #{isShow},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from merchant_voucher_manager where id = #{id}
    </delete>
</mapper>